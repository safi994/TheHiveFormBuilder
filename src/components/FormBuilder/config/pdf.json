{
  "styles": {
    "page": {
      "backgroundColor": "#ffffff",
      "padding": 30,
      "color": "#1f2937"
    },
    "title": {
      "fontSize": 24,
      "marginBottom": 20,
      "fontWeight": "bold",
      "color": "#111827"
    },
    "row": {
      "flexDirection": "row",
      "marginBottom": 6,
      "width": "100%",
      "gap": 0,
      "display": "flex"
    },
    "section": {
      "padding": "4 4",
      "marginRight": 0,
      "marginBottom": 4,
      "marginLeft": 0
    },
    "label": {
      "fontSize": 12,
      "marginBottom": 4,
      "color": "#374151",
      "fontWeight": "medium"
    },
    "value": {
      "fontSize": 12,
      "color": "#1f2937"
    },
    "required": {
      "color": "#dc2626"
    },
    "elements": {
      "checkbox": {
        "width": 12,
        "height": 12,
        "border": "1px solid #6b7280",
        "marginRight": 8
      },
      "select": {
        "padding": 4,
        "marginTop": 0,
        "minHeight": 16,
        "border": "1px solid #d1d5db",
        "fontSize": 12,
        "color": "#1f2937"
      },
      "input": {
        "padding": 4,
        "marginTop": 0,
        "minHeight": 16,
        "border": "1px solid #d1d5db",
        "fontSize": 12,
        "color": "#1f2937"
      }
    }
  }
}